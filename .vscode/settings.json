        "titleBar.activeBackground": "#830000", // タイトルバーの背景色
        "titleBar.activeForeground": "#FFFFFF", // タイトルバーの文字色
        "statusBar.background": "#1ca5ef", // ステータスバーの背景色
        "statusBar.foreground": "#000000", // ステータスバーの文字色
